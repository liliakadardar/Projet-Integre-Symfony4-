{% extends 'base-back.html.twig' %}

{% block title %}New Commande Evenement{% endblock %}

{% block body %}
<div class="content-body">
    <div class="container-fluid">
        <div class="page-titles">
    <h4>AjouterCommande Evenement</h4>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="javascript:void(0)">Commande Evenement</a></li>
                <li class="breadcrumb-item active"><a href="javascript:void(0)">Ajouter Commande Evenement</a></li>
            </ol>
        </div>
        <div class="col-lg-12">

        <div class="card">
            <div class="card-header">
            </div>
            <div class="card-body">
                <div class="basic-form">
                    {{ form_start(form,{'attr': {'novalidate': 'novalidate'}} ) }}
                    <div id="wizard_Service" class="tab-pane" role="tabpanel">
                        <div class="row">
                            <div class="col-lg-6 mb-2">
                                <div class="form-group">
                                    <div class="form-group">
                                        <label class="text-label">Date creation</label>
                                        {{ form_widget(form.date_creation,{'attr':{ 'class':'form-control input-default' ,'placeholder':'Date'}}) }}
                                        {{ form_errors(form.date_creation) }}
                                    </div>
                                    <div class="form-group">
                                        <label class="text-label">Address destination</label>
                                        {{ form_widget(form.address_destination,{'attr':{'class':'form-control input-default', 'placeholder':'Address destination'}}) }}
                                        {{ form_errors(form.address_destination) }}
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <input type="submit" class="btn mr-2 btn-primary" value="Ajouter">
            <a type="button" href="{{ path('commande_e_index_admin') }}"  class="btn btn-rounded btn-outline-warning">back to list</a>

            {{ form_end(form) }}

        </div>
    </div>
</div>
    </div>
   {#  {{ include('commande_e/back/_form.html.twig') }}

    <a href="{{ path('commande_e_index_admin') }}">back to list</a> #}
{% endblock %}
